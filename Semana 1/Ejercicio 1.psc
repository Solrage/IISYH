Proceso Ejercicio_1
	Escribir "Ingrese radio del cilindro"
	leer radio
	escribir "ingrese altura del cilindro"
	leer altura
	AreaCilindro <- 2*PI*radio*(altura+radio) 
	VolumenCilindro <- PI*radio^2*altura
	Escribir "El Area total es:", AreaCilindro
	escribir "El volumen del cilindro:", VolumenCilindro
FinProceso
